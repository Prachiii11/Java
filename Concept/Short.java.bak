class Short
{
	public static void main(String[] args) 
	{
	    short n =100;

		int n2=n;
		System.out.println(n2);

		long n3=n;
		System.out.println(n3);

		float n4=n;
		System.out.println(n4);

		double n5=n;
		System.out.println(n5);


		char ch=n;
		System.out.println(ch);

	}
}
